{
  "name": "@tager/web",
  "version": "1.0.0",
  "private": true,
  "main": "server-build/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build-server": "rimraf server-build && babel server -d server-build --extensions \".ts,.json\" ",
    "start-server": "node server-build/index.js",
    "build": "yarn build-server && next build && yarn build-storybook",
    "start": "yarn build-server && cross-env NODE_ENV=development yarn start-server",
    "serve": "cross-env NODE_ENV=production yarn start-server",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "ci": "start-server-and-test start http://localhost:3000 cypress:run"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.4.0",
    "@sentry/browser": "^5.18.0",
    "@sentry/node": "^5.18.0",
    "@sentry/webpack-plugin": "^1.11.1",
    "@tager/web-components": "^0.1.3",
    "@tager/web-core": "^0.1.2",
    "@zeit/next-source-maps": "0.0.4-canary.1",
    "body-scroll-lock": "^3.0.3",
    "express": "^4.17.1",
    "formik": "^2.1.4",
    "lodash": "^4.17.15",
    "next": "^9.4.4",
    "next-i18next": "^4.5.0",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "react": "^16.13.1",
    "react-app-polyfill": "1.0.6",
    "react-dom": "^16.13.1",
    "react-gtm-module": "^2.0.8",
    "react-redux": "^7.2.0",
    "sitemap": "^6.1.6",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.3",
    "@cypress/webpack-preprocessor": "^5.4.1",
    "@storybook/react": "^5.3.19",
    "@svgr/webpack": "5.4.0",
    "@testing-library/cypress": "^6.0.0",
    "@types/body-scroll-lock": "^2.6.1",
    "@types/express": "^4.17.6",
    "@types/lodash": "^4.14.157",
    "@types/node": "12.x",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "@types/react-gtm-module": "^2.0.0",
    "@types/react-redux": "7.1.9",
    "@types/styled-components": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "2.x",
    "@typescript-eslint/parser": "2.x",
    "babel-eslint": "10.x",
    "babel-loader": "^8.1.0",
    "babel-preset-react-app": "^9.1.2",
    "cross-env": "^7.0.2",
    "cypress": "4.9.0",
    "eslint": "6.x",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-flowtype": "4.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-react-hooks": "2.x",
    "fs-extra": "^9.0.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "next-compose-plugins": "^2.2.0",
    "next-transpile-modules": "^3.3.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "start-server-and-test": "^1.11.0",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.5",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0"
  },
  "resolutions": {
    "cypress": "4.9.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
